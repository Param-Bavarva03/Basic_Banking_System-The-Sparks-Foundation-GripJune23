    <div class="addusercont2">
        <div class="container mt-5 bg-primary text-center text-light fw-30 p-2">
            <h3>Add User Form</h3>
        </div>
        <div class="adduser_class d-flex flex-row align-items-center">
    
            <div class="userimg">
                <img src="./img/add-user-icon.jpg" class="img-fluid" id="img2" alt="img2">
            </div>
    
            <form action="/AddUser" method="POST" class="userform">
                <div class="form-group mt-3">
                    <label for="exampleInputEmail1">Name</label>
                    <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                        name="name" placeholder="Enter User Name" required>
    
                </div>
                <div class="form-group  mt-3">
                    <label for="exampleInputEmail1">Email address</label>
                    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                        name="email" placeholder="Enter User Email">
    
                </div>
                <div class="form-group  mt-3">
                    <label for="exampleInputPassword1">Mobile</label>
                    <input type="text" class="form-control" id="exampleInputPassword1" name="mobile"
                        placeholder="Enter User Contact ">
    
                </div>
                <div class="form-group  mt-3">
                    <label for="exampleInputEmail1">Amount</label>
                    <input type="number" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                        name="amount" placeholder="Enter Amount">
    
                    <button type="submit" class="btn btn-primary mt-3 text-center">Submit</button>
            </form>
        </div>
    </div>


    <div class="foot">
        <footer class="text-center text-lg-start bg-dark text-muted text-light cus-foot">
